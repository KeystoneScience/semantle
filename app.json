    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.nateastone.FlixPix",
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "1.0.5",
      "appStoreUrl": "https://itunes.apple.com/us/app/flixpix/id1561264727",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-9160266233776800~1854305208"
      },
      "associatedDomains": [
        "applinks:*.flixpix.app",
        "applinks:flixpix.app",
        "applinks:www.flixpix.app",
        "webcredentials:*.flixpix.app",
        "webcredentials:www.flixpix.app"
      ]
    },
    "android": {
      "versionCode": 5,
      "package": "com.nateastone.FlixPix",
      "googleServicesFile": "./google-services.json",
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.nateastone.FlixPix",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/logos/icon.png",
        "backgroundColor": "#B1B1B1"
      },
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-9160266233776800~5446922080"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "*.flixpix.app",
              "pathPrefix": "/deeplink"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./app/assets/logos/icon.png"
    },
    "description": "Alpha 1.0.5.1 added in search screen"
  }
