    "orientation": "portrait",
    "icon": "./app/assets/logos/icon.png",
    "splash": {
      "image": "./app/assets/logos/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
